{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_PAYPAL_CLIENT_ID": "@next_public_paypal_client_id",
    "NEXTAUTH_URL": "@nextauth_url",
    "NEXTAUTH_SECRET": "@nextauth_secret",
    "NEXT_PUBLIC_APP_ENV": "production"
  },
  "functions": {
    "app/page.tsx": {
      "includeFiles": "public/**"
    }
  },
  "regions": ["cle1"],
  "framework": "nextjs"
}
